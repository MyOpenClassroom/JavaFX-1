      <Label fx:id="theSecondExceptionLabel" layoutX="376.0" layoutY="132.0" text="ТОЛЬКО ЧИСЛА!" />
      <ImageView fitHeight="88.0" fitWidth="520.0" layoutX="10.0" layoutY="290.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/images/Безымянный.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
