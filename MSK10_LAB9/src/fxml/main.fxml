<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="384.0" prefWidth="533.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Controller">
   <children>
      <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="274.0" prefWidth="302.0">
        <columns>
          <TableColumn fx:id="col1" prefWidth="150.0" text="K(i)" />
          <TableColumn fx:id="col2" prefWidth="150.0" text="Y(i)" />
        </columns>
      </TableView>
      <Button fx:id="buttonStart" layoutX="324.0" layoutY="74.0" mnemonicParsing="false" onAction="#buttonStartAction" prefWidth="200.0" text="Выполнить задание" />
      <TextField fx:id="aTextField" layoutX="349.0" layoutY="160.0" />
      <Label layoutX="326.0" layoutY="164.0" text="a = " />
      <TextField fx:id="bTextField" layoutX="349.0" layoutY="195.0" />
      <Label layoutX="326.0" layoutY="199.0" text="b = " />
      <Button fx:id="buttonRandom" layoutX="324.0" layoutY="39.0" mnemonicParsing="false" onAction="#buttonRandomAction" prefWidth="200.0" text="Заполнить случайными числами" />
      <Button fx:id="buttonCleanUp" layoutX="329.0" layoutY="243.0" mnemonicParsing="false" onAction="#buttonCleanUpAction" prefHeight="25.0" prefWidth="94.0" text="Очистка" />
